.border-bottom {
  position: relative;
  border: none;
}

.border-bottom:after {
  content: '';
  position: absolute;
  bottom: 0;
  background: rgb(237, 237, 237);
  width: 85%;
  height: 1px;
  right: 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
}

.alphanet-selector {
  position: absolute;
