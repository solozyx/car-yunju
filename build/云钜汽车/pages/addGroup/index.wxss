}

.m-body-bottom {
  height: 80rpx;
  text-align: center;
}

.m-body-bottom text {
  font-size: 24rpx;
  color: #888;
}

.modai-close {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 125rpx;
  margin: 0 auto;
  text-align: center;
}

.tabbar-bottom {
